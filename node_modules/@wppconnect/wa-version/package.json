{
  "name": "@wppconnect/wa-version",
  "version": "1.2.2",
  "description": "List of WhatsApp Web HTML",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Edgard Messias",
    "email": "edgardmessias@gmail.com",
    "url": "https://github.com/edgardmessias"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build:dev": "tsc --sourceMap",
    "build": "tsc",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean": "shx rm -rf session dist",
    "commit": "cz",
    "lint": "npx eslint --ext .ts src",
    "prepare": "husky install && npm run clean && npm run build",
    "release": "release-it",
    "wa-update": "ts-node ./src/tools/update.ts",
    "watch": "tsc -w --sourceMap"
  },
  "dependencies": {
    "node-fetch": "^2.6.9",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@types/node": "^14.18.36",
    "@types/node-fetch": "^2.6.2",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "~5.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "release-it": "^15.6.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
